<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:layout_width="300dp"
        android:layout_height="120dp"
        android:id="@+id/Image_Title"
        android:layout_gravity="center"
        android:src="@drawable/icon_otherurltitle"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:onClick="onBtnClicked_other_url_5500"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="5500-1번"
        android:id="@+id/Button_Url_5500"
        android:textStyle="bold"
        android:textSize="40sp"
        android:layout_marginTop="50dp"
        android:background="#00000000"
        android:singleLine="false"
        android:layout_below="@+id/Image_Title"
        android:layout_centerHorizontal="true" />

    <Button
    android:onClick="onBtnClicked_other_url_1112"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:text="1112번"
    android:id="@+id/Button_Url_1112"
    android:textStyle="bold"
    android:textSize="40sp"
    android:layout_marginTop="20dp"
    android:background="#00000000"
    android:singleLine="false"
    android:layout_below="@+id/Button_Url_5500"
    android:layout_centerHorizontal="true" />

    <Button
    android:onClick="onBtnClicked_other_url_5100"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:text="5100번"
    android:id="@+id/Button_Url_5100"
    android:textStyle="bold"
    android:textSize="40sp"
    android:layout_marginTop="20dp"
    android:background="#00000000"
    android:singleLine="false"
    android:layout_below="@+id/Button_Url_1112"
    android:layout_centerHorizontal="true" />

    <Button
        android:onClick="onBtnClicked_other_url_M5107"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="M5107번"
        android:id="@+id/Button_Url_M5107"
        android:textStyle="bold"
        android:textSize="40sp"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:singleLine="false"
        android:layout_below="@+id/Button_Url_5100"
        android:layout_centerHorizontal="true" />

    <Button
        android:onClick="onBtnClicked_other_url_9"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="9번"
        android:id="@+id/Button_Url_9"
        android:textStyle="bold"
        android:textSize="40sp"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:singleLine="false"
        android:layout_below="@+id/Button_Url_M5107"
        android:layout_centerHorizontal="true" />

</RelativeLayout>